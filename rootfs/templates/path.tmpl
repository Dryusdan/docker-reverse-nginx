  location {{frontend_path}} {
    include /nginx/conf.d/proxy.conf;
    {% if frontend_auth != "" %}
      auth_basic "Who's this?";
      auth_basic_user_file "{{auth_file}}";
    {% endif %}

    client_max_body_size {{frontend_domain_max_body_size}};
    proxy_pass http://{{backend_addr}}:{{backend_port}};
  }